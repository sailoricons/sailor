{
  "name": "sailor-icons",
  "version": "1.0.0",
  "description": "Nice open source icons",
  "main": "dist/sailor.js",
  "unpkg": "dist/sailor.min.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "all": "npm-run-all --sequential build lint test:coverage",
    "build": "./bin/build.sh",
    "lint": "eslint .",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "cm": "git-cz",
    "precommit": "lint-staged",
    "commitmsg": "commitlint --edit"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "core-js": "^3.1.3"
  },
  "devDependencies": {
    "@babel/cli": "7.6.0",
    "@babel/core": "7.6.0",
    "@babel/node": "7.6.1",
    "@babel/preset-env": "7.6.0",
    "@commitlint/cli": "^5.2.5",
    "@commitlint/config-conventional": "^6.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.6",
    "babel-preset-stage-2": "^6.24.1",
    "cheerio": "^1.0.0-rc.2",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.5.0",
    "eslint-plugin-prettier": "^2.5.0",
    "html-minifier": "^3.5.8",
    "husky": "^0.14.3",
    "jest": "24.9.0",
    "lint-staged": "^6.0.0",
    "npm-run-all": "^4.1.2",
    "prettier": "1.18.2",
    "semantic-release": "^12.2.2",
    "svgo": "^0.7.2",
    "webpack": "4.40.2",
    "webpack-cli": "3.3.8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sailoricons/sailor.git"
  },
  "author": "Catalin Besleaga <catalinbesleagadev@gmail.com> (https://catalinbesleaga.co)",
  "license": "MIT"
}
